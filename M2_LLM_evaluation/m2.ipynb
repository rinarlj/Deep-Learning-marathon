      "source": [
        "def format_prompt(question, choices):\n",
        "    formatted_choices = \"\\n\".join([f\"{i+1}. {choice}\" for i, choice in enumerate(choices)])\n",
        "    return f\"Question: {question}\\nChoices:\\n{formatted_choices}\\nAnswer:\"\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 17,
      "metadata": {
        "id": "rShQDxPZqUDA"
      },
      "outputs": [],
      "source": [
        "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 9,
      "metadata": {
        "id": "XOOsFMoIKE2u"
      },
      "outputs": [],
      "source": [
        "\n",
        "def evaluate_model(model, tokenizer, dataset):\n",
        "    correct = 0\n",
        "    total = len(dataset)\n",
        "\n",
        "    for item in dataset:\n",
        "        prompt = format_prompt(item[\"question\"], item[\"choices\"])\n",
        "\n",
        "\n",
        "        inputs = tokenizer(prompt, return_tensors=\"pt\", truncation=True, max_length=512)\n",
        "\n",
        "        outputs = model.generate(**inputs, max_new_tokens=10)\n",
        "        generated_answer = tokenizer.decode(outputs[0], skip_special_tokens=True)\n",
        "\n",
        "        generated_answer = generated_answer.split(\"\\n\")[-1].strip()\n",
        "\n",
        "        if generated_answer == item[\"choices\"][item[\"answer\"]]:\n",
        "            correct += 1\n",
        "\n",
        "    accuracy = correct / total\n",
        "    return accuracy\n"
      ]
    },
